        "@typescript-eslint/parser": "^5.12.0",
        "@vue/compiler-sfc": "^3.2.31",
        "alpinejs": "^3.4.2",
        "axios": "^0.25",
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-vue": "^8.4.1",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.6",
        "prettier": "^2.5.1",
        "resolve-url-loader": "^5.0.0",
